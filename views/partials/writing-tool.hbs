<div id='writing-tool'>
  <div class='tool-group'>
    {{#each modificationOptions}}
      <button 
        class='clova-cmd' 
        data-value='{{value}}' 
        data-length='{{length}}'
        data-position='{{position}}'
        data-stream='{{stream}}'
      >
        {{label}}
      </button>
    {{/each}}
    <button id='tool-cancel-btn'>취소</button>
  </div>
  {{> editor-box }}
  {{> sub-alert-popup }}
</div>